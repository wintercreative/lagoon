{
  "name": "api",
  "version": "3.0.0",
  "description": "Lagoon GraphQL API",
  "main": "./dist/index.js",
  "engines": {
    "node": ">=10.16.3"
  },
  "scripts": {
    "format": "prettier-eslint --write '**/*.js'",
    "lint": "../../node-packages/eslint-config-lagoon-node/node_modules/.bin/eslint .",
    "test": "jest --forceExit --detectOpenHandles --maxWorkers=10",
    "test:watch": "jest --forceExit --detectOpenHandles --maxWorkers=10 --watch",
    "build": "tsc --pretty",
    "start": "ts-node-dev --inspect=0.0.0.0:9229 --ignore-watch node_modules --no-notify --respawn --transpileOnly src/index.ts",
    "dev-archive": "NODE_ENV=development nodemon --ignore '*.test.ts' --watch src -e js,json,ts --exec 'node --inspect=0.0.0.0:9229 --require ts-node/register src/index.ts -r dotenv-extended/config' -- dist",
    "sync:gitlab:users": "flow-node dist/gitlab-sync/users",
    "sync:gitlab:groups": "flow-node dist/gitlab-sync/groups",
    "sync:gitlab:projects": "flow-node dist/gitlab-sync/projects",
    "sync:gitlab:all": "yarn run sync:gitlab:users && yarn run sync:gitlab:groups && yarn run sync:gitlab:projects",
    "sync:opendistro-security": "flow-node --max-http-header-size=80000 dist/helpers/sync-groups-opendistro-security"
  },
  "keywords": [],
  "author": "amazee.io <hello@amazee.io> (http://www.amazee.io)",
  "license": "MIT",
  "dependencies": {
    "@lagoon/commons": "4.0.0",
    "apollo-server-express": "^2.9.9",
    "aws-sdk": "^2.378.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "camelcase-keys": "^4.2.0",
    "class-validator": "^0.11.0",
    "compression": "^1.7.1",
    "cors": "^2.8.5",
    "dataloader": "^1.4.0",
    "dotenv": "^4.0.0",
    "dotenv-extended": "^2.0.1",
    "elasticsearch": "^15.0.0",
    "es7-sleep": "^1.0.0",
    "express": "^4.17.1",
    "flow-remove-types": "^1.2.3",
    "got": "^9.2.2",
    "graphql": "^14.5.8",
    "graphql-custom-types": "^1.5.1",
    "graphql-iso-date": "^3.5.0",
    "graphql-list-fields": "^2.0.2",
    "graphql-middleware": "^4.0.2",
    "graphql-rabbitmq-subscriptions": "^1.1.0",
    "graphql-subscriptions": "^1.0.0",
    "graphql-tools": "^4.0.6",
    "graphql-type-json": "^0.3.0",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^8.5.1",
    "keycloak-admin": "https://github.com/amazeeio/keycloak-admin.git\\#allow-more-customization",
    "keycloak-connect": "^8.0.0",
    "keycloak-connect-graphql": "^0.2.2",
    "knex": "^0.14.3",
    "mariasql": "^0.2.6",
    "moment": "^2.24.0",
    "morgan": "^1.9.0",
    "mysql": "^2.17.1",
    "node-cache": "^4.2.1",
    "pg": "^7.13.0",
    "ramda": "^0.25.0",
    "reflect-metadata": "^0.1.10",
    "snakecase-keys": "^1.2.0",
    "sqlite3": "^4.1.0",
    "sshpk": "^1.14.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "type-graphql": "^0.17.5",
    "typeorm": "0.2.20",
    "validator": "^10.8.0",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.17.1",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/faker": "^4.1.5",
    "@types/helmet": "^0.0.45",
    "@types/jest": "^24.0.18",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/node": "^12.12.8",
    "@types/ramda": "types/npm-ramda#dist",
    "axios": "^0.19.0",
    "faker": "^4.1.0",
    "jest": "^24.9.0",
    "nodemon": "^1.19.4",
    "prettier": "^1.14.2",
    "prettier-eslint-cli": "^4.7.1",
    "ts-jest": "^24.0.2",
    "ts-node": "8.5.2",
    "typescript": "3.7.2"
  }
}